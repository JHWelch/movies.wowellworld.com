<div
  x-init="() => {
    const urlParams = new URLSearchParams(window.location.search);
    if (urlParams.has('suggest_success')) {
      confetti();
    }
  }"
  x-data="{
    confetti: function () {
      const jsConfetti = new JSConfetti()

      jsConfetti.addConfetti({
        emojis: [
          '🎄',
          '🍪',
          '☃️',
          '🌟',
          '🎁',
          '🎅',
          '🤶',
          '🌰',
          '🦌',
          '🧝',
          '👼',
          '🧦',
          '🔔',
          '❄️',
        ],
      })
    }
  }"
  class="hidden"
  x-on:fire-confetti.window="confetti()"
></div>
