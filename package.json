{
  "name": "movies.wowellworld.com",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm --prefix server install && npm --prefix client install",
    "lint": "eslint",
    "fix": "npm run lint -- --fix",
    "dev": "concurrently -k -c \"#e7b885,#53ceff,#a4d4b8\"  \"npm run dev:firebase\" \"sleep 2 && vite\" --names=firebase,vite",
    "dev:server": "npm run --prefix server dev",
    "dev:firebase": "npm run --prefix server firebase",
    "dev:client": "npm run --prefix client dev",
    "dev:export": "npm run --prefix server firebase:export",
    "build": "vue-tsc -p tsconfig.build.json && vite build && npm run build:emails && npm run link:emails",
    "build:emails": "mkdir -p ./emails/built && mjml --config.minify=true ./emails/*.mjml -o ./emails/built/",
    "preview": "vite preview",
    "clean" : "rm -rf built",
    "link:emails": "ln -s ../emails built/emails",
    "start": "NODE_ENV=production tsx src/server/index.ts",
    "test": "vitest",
    "types": "vue-tsc --noEmit",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "notion",
    "movies"
  ],
  "author": "Jordan Welch",
  "license": "MIT",
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "js-confetti": "^0.13.1",
    "lodash.debounce": "^4.0.8",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@fetch-mock/vitest": "^0.2.15",
    "@jhwelch/eslint-config": "^4.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.18",
    "@types/lodash.debounce": "^4.0.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.14.0",
    "jsdom": "^27.0.0",
    "mjml": "^4.18.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vite": "^7.1.11",
    "vitest": "^3.1.3",
    "vue-tsc": "^3.0.7"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.54.0"
  }
}
