{
  "name": "notion-movie-page-generator",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "npm run build-ts && npm run build-tailwind",
    "start": "node built/index.js",
    "build-ts": "tsc",
    "start-ts": "ts-node src/index.ts",
    "build-tailwind": "npx tailwindcss -i ./css/app.css -o ./public/app.css --minify",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "watch": "concurrently \"npm run watch-express\" \"npm run watch-tailwind\"",
    "watch-express": "nodemon src/index.ts",
    "watch-tailwind": "npx tailwindcss -i ./css/app.css -o ./public/app.css --watch"
  },
  "keywords": [
    "notion",
    "movies"
  ],
  "author": "Jordan Welch",
  "license": "MIT",
  "nodemonConfig": {
    "ext": "ts,ejs",
    "execMap": {
      "ts": "ts-node --esm"
    }
  },
  "dependencies": {
    "@notionhq/client": "^2.2.5",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.19",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "firebase": "^10.1.0",
    "firebase-admin": "^11.10.1",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@jest/globals": "^29.5.0",
    "@tailwindcss/typography": "^0.5.9",
    "@tsconfig/node18": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
