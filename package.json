{
  "name": "notion-movie-page-generator",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "npx tailwindcss -i ./css/app.css -o ./public/app.css --minify",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "start": "node src/index.js",
    "test": "jest",
    "watch": "concurrently \"npm run watch-express\" \"npm run watch-tailwind\"",
    "watch-express": "nodemon npm start",
    "watch-tailwind": "npx tailwindcss -i ./css/app.css -o ./public/app.css --watch"
  },
  "keywords": [
    "notion",
    "movies"
  ],
  "author": "Jordan Welch",
  "license": "MIT",
  "nodemonConfig": {
    "ext": "js,ejs"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.5",
    "@types/node": "^18.16.19",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tailwindcss/typography": "^0.5.9",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6"
  }
}
