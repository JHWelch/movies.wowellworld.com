{
  "name": "movies.wowellworld.com",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "main": "src/server/main.ts",
  "scripts": {
    "cli": "tsx src/server/cli.ts",
    "lint": "eslint",
    "fix": "npm run lint -- --fix",
    "dev": "concurrently -k -c \"#e7b885,#53ceff\"  \"npm run firebase\" \"sleep 2 && npm run dev:app\" --names=firebase,app",
    "dev:app": "nodemon -w src/server -x tsx src/server/main.ts",
    "firebase": "firebase emulators:start --import emulator-cache",
    "firebase:export": "firebase emulators:export emulator-cache",
    "build": "vite build && npm run build:emails",
    "build:emails": "mkdir -p ./emails/built && mjml --config.minify=true ./emails/*.mjml -o ./emails/built/",
    "preview": "vite preview",
    "clean" : "rm -rf dist",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "test": "vitest",
    "types": "vue-tsc -p tsconfig.build.json --noEmit",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "notion",
    "movies"
  ],
  "author": "Jordan Welch",
  "license": "MIT",
  "dependencies": {
    "@jhwelch/made-with": "^0.0.3",
    "@heroicons/vue": "^2.2.0",
    "@notionhq/client": "^5.0.0",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.2.1",
    "firebase-admin": "^13.5.0",
    "firebase": "^12.5.0",
    "ics": "^3.8.1",
    "js-confetti": "^0.13.1",
    "lodash.debounce": "^4.0.8",
    "luxon": "^3.7.2",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "vite-express": "^0.22.0",
    "vue-router": "^4.6.4",
    "vue": "^3.5.26",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@fetch-mock/vitest": "^0.2.15",
    "@jhwelch/eslint-config": "^5.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.18",
    "@types/express": "^5.0.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.0",
    "@vue/test-utils": "^2.4.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.14.0",
    "firebase-tools": "^15.3.1",
    "jsdom": "^27.0.0",
    "mjml": "^4.18.0",
    "mockdate": "^3.0.5",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.0.0",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.4",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vite": "^7.1.11",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.0.7"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.54.0"
  }
}
